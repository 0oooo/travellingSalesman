package travellingSalesman;

public class Road {
	
	Object[] road;
	
	Road(City city1, City city2){
		road = new Object[3];
		double distance = city1.calculateDistanceCities(city2);
		road[0] = city1; 
		road[1] = city2;
		road[2] = distance; 
	}
	
	
	public int getCity1() {
		return (int)road[0];
	}
	
	public int getCity2() {
		return (int)road[1];
	}
	
	public double getDistance() {
		return (double)road[2];
	}
}
